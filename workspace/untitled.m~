load angle.mat a
load data.mat data_2
data = CTplot(data_2,a,-9.1185,-6.2401,0.2774,1);
A = imrotate(data,-5);

se=translate(strel(1),[-80 -80]);
img2=imdilate(A,se);
imagesc(img2);
img2(img2 > 0) = 1;
img2(img2 < 0) = 0;

NewImg = [];

for i = 1 : 360
    for j = 1 : 360
        NewImg(i, j) = img2(181, 181);
    end
end